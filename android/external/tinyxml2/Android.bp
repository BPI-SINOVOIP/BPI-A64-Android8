cc_library {
    name: "libtinyxml2",
    host_supported: true,
    vendor_available: true,
    vndk: {
        enabled: true,
    },

    srcs: ["tinyxml2.cpp"],

    shared_libs: ["liblog"],

    target: {
        android: {
            cflags: [
                // LOG_TO_ANDROID_LOGCAT
                "-DDEBUG",
                "-DANDROID_NDK",
            ],
        },
    },

    export_include_dirs: ["."],
}
