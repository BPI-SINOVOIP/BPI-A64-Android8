<configuration description="Cluster TF config for ATP lab machines running adb tests">
    <command_scheduler class="com.google.android.adb.AdbTestClusterCommandScheduler" />
    <device_manager class="com.android.tradefed.device.DeviceManager">
        <option name="max-null-devices" value="1" />
    </device_manager>
    <device_monitor class="com.google.android.tradefed.cluster.ClusterDeviceMonitor">
        <option name="host-info-cmd" key="prodcertstatus" value="prodcertstatus" />
        <option name="host-info-cmd" key="krbstatus" value="krbstatus" />
        <option name="host-info-cmd-timeout" value="10000" />
    </device_monitor>
    <object type="cluster_client" class="com.google.android.tradefed.cluster.ClusterClient" />
    <object type="cluster_options" class="com.google.android.tradefed.cluster.ClusterOptions" />
</configuration>