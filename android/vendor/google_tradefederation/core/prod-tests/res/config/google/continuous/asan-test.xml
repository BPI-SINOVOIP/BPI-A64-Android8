<configuration description="ASAN Status Test">
    <option name="test-tag" value="asan-test" />

    <target_preparer class="com.android.tradefed.targetprep.PushFilePreparer">
        <option name="cleanup" value="true" />
        <option name="push" value="asan_test->/data/local/tmp/asan_test" />
    </target_preparer>

    <test class="com.android.tradefed.testtype.GTest">
        <option name="native-test-device-path" value="/data/local/tmp" />
        <option name="module-name" value="asan_test" />
        <option name="negative-testname-filter" value="Global" />
    </test>
</configuration>
