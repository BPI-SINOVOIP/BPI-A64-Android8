<!-- This is a base config intended to be included by other test configs. -->
<!-- Not intended to be run by itself. -->
<configuration description="Base args for camera stress tests.">
    <include name="google/camera/camera_base" />
    <include name="google/test/framework/media/camera2-stress" />

    <option name="test-package" value="com.android.camera2" />
    <option name="instrumentation-arg" key="screenshotOnFailure" value="true" />
    <option name="test-file-name" value="GoogleCameraTests.apk" />
</configuration>
