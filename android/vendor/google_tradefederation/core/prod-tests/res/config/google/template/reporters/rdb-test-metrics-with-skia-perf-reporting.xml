<!-- Result reports for reporting test metrics to Android and skia  -->
<configuration description="Result reports for reporting test metrics to RDB" >
    <include name="google/template/reporters/rdb-test-metrics" />
    <result_reporter class="com.android.tradefed.result.JsonHttpTestResultReporter">
