<configuration description="Set location provider to gps">
  <device name="watch">
    <option name="google-device-setup:location-opt-in" value="on" />
    <option name="google-device-setup:location-network" value="off" />
    <option name="google-device-setup:location-gps" value="on" />
    <option name="google-device-setup:wifi" value="off" />
  </device>
</configuration>
