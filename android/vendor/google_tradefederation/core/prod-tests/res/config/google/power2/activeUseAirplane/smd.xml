<!-- Required input: monsoon serialno -->
<configuration description="Downloads build, flashes on device, and runs power test">
    <include name="google/power2/settings/airplane" />
    <option name="test-tag" value="powerSMD" />
    <test class="com.google.android.power.tests.PowerTestRunner">
        <option name="report_unit_mw" value="true" />
        <option name="test_case_name" value="testTriggerMotion" />
        <option name="test_class_name" value="SignificantMotionTests" />
        <option name="test_package_name" value="com.google.android.powertests" />
        <option name="test_runner_name" value="android.test.InstrumentationTestRunner" />
        <option name="monsoon_voltage" value="4.2" />
        <option name="monsoon_samples" value="9000" />
        <option name="ru_name" value="ActiveUseAirplane" />
    </test>
</configuration>
