<configuration description="Install apks needed most power tests on watch">
  <!-- Installing APK on the watch side-->
  <device name="watch">
    <target_preparer class="com.android.tradefed.targetprep.TestAppInstallSetup">
        <option name="alt-dir" value="/google/data/ro/teams/tradefed/testdata/power" />
        <option name="test-file-name" value="Power.apk" />
      </target_preparer>

      <target_preparer class="com.android.tradefed.targetprep.InstallApkSetup" />
  </device>
</configuration>
