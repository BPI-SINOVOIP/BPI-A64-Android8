<!-- Result reports for reporting run metrics to RDB. -->
<configuration description="Result reports for reporting run metrics to RDB">
    <result_reporter class="com.google.android.tradefed.result.PowerRunMetricsReporter">
      <option name="report-on-failure" value="true"/>
    </result_reporter>
    <result_reporter class="com.android.tradefed.result.JsonHttpTestResultReporter">
        <!-- disabled by default -->
        <option name="disable" value="true" />
        <option name="posting-endpoint" value="https://android-master-ingest.skia.org/upload" />
    </result_reporter>
</configuration>
