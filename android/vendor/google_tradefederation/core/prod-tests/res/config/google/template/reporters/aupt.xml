<!-- Common loggers, log savers, and result reporters for AUPT templates. -->
<configuration description="Common loggers, log savers, and result reporters for AUPT templates" >
    <result_reporter class="com.google.android.aupt.AuptLogcatAnalysisReporter" />
    <result_reporter class="com.google.android.aupt.AuptInspectBugReporter">
        <option name="suite-key" value="aupt2-heavy" />
        <option name="suite-instance-key" value="aupt2"/>
    </result_reporter>
    <result_reporter class="com.google.android.tradefed.result.BlackboxResultReporter" />
</configuration>
