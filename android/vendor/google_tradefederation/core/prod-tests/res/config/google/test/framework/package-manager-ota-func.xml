<configuration description="Runs download manager host-based tests">
    <option name="test-tag" value="package_manager_ota" />

    <!-- Used to forward the test dir to the test as part of the config. -->
    <target_preparer class="com.android.framework.tests.TestDirForwarder" />

    <test class="com.android.framework.tests.PackageManagerOTATests">
        <option name="use-priv-path" value="true" />
    </test>
</configuration>
