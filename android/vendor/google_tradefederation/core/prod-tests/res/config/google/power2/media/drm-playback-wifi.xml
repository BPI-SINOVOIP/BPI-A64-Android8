<!-- Required input: monsoon_serialno monsoonserialno -->
<configuration description="Downloads build, flashes on device, and runs power test">
    <include name="google/power2/settings/wifi" />
    <option name="test-tag" value="powerDRMplaybackWifi" />
    <target_preparer class="com.android.tradefed.targetprep.PushFilePreparer" >
        <option name="push" value="/google/data/ro/teams/tradefed/testdata/power/WVInfoStreamingTV.txt->/sdcard/WVInfo.txt" />
    </target_preparer>
    <test class="com.google.android.power.tests.PowerTestRunner">
        <option name="report_unit_mw" value="true" />
        <option name="test_case_name" value="testWVPlayback" />
        <option name="test_class_name" value="WideVineDrmPowerTest" />
        <option name="test_package_name" value="com.google.android.powertests" />
        <option name="test_runner_name" value="android.test.InstrumentationTestRunner" />
        <option name="monsoon_voltage" value="4.2" />
        <option name="monsoon_samples" value="10000" />
        <option name="ru_name" value="powerMedia" />
    </test>
</configuration>
