<configuration description="Bluetooth stress tests">
    <option name="test-tag" value="bluetooth_stress" />

    <target_preparer class="com.android.tradefed.targetprep.TestAppInstallSetup">
        <option name="test-file-name" value="BluetoothTests.apk" />
    </target_preparer>
    <!-- Left blank so some configs can add their own run command -->
