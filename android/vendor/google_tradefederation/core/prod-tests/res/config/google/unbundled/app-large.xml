<!-- Required options: branch build-flavor app-package-name package -->
<configuration description="Installs unbundled app, runs large tests">
    <include name="google/unbundled/common/app-func" />
    <!-- build provider options -->
    <option name="test-tag" value="LargeSuite" />

    <test class="com.android.tradefed.testtype.InstalledInstrumentationsTest" >
        <option name="size" value="large" />
        <option name="test-timeout" value="120000" />
    </test>

    <!-- result reporter options -->
    <!--  2 is large -->
    <option name="sponge:sponge-size" value="2" />
</configuration>
