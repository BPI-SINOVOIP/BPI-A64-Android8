<configuration description="Runs ClusterFuzz Fuzzing Framework">
    <option name="all-devices" value="true" />
    <option name="min-battery" value="30" />
    <option name="cutoff-battery" value="10" />
    <option name="loop" value="true" />
    <option name="test-tag" value="clusterfuzz" />

    <test class="com.google.android.clusterfuzz.ClusterFuzzTest" />

    <logger class="com.android.tradefed.log.FileLogger" />
    <log_saver class="com.android.tradefed.result.FileSystemLogSaver">
        <option name="log-file-path" value="/auto/android-test/www/test_logs" />
        <option name="log-file-url" value="http://android-test/www/test_logs" />
        <option name="log-retention-days" value="30" />
    </log_saver>
    <result_reporter class="com.google.android.tradefed.result.SpongeResultReporter" />
</configuration>
