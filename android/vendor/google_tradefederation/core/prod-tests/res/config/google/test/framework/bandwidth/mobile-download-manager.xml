<configuration description="Bandwidth download microbenchmark tests for mobile using Download Manager">
    <option name="test-tag" value="bandwidth_mobile_download_manager" />

    <target_preparer class="com.android.tradefed.targetprep.TestAppInstallSetup">
        <option name="test-file-name" value="BandwidthTests.apk" />
    </target_preparer>

    <test class="com.android.framework.tests.BandwidthMicroBenchMarkTest">
        <option name="test-package-name" value="com.android.bandwidth.tests" />
        <option name="test-class-name" value="com.android.bandwidthtest.BandwidthTest" />
        <option name="test-method-name" value="testMobileDownloadWithDownloadManager" />
        <option name="test-label" value="BandwidthMobileTestDownloadManager" />
        <option name="bandwidth-test-server" value="http://test-bandwidth.appspot.com/" />
        <option name="compact-ru-key" value="mobile-dlmgr-pass-fail" />
    </test>
</configuration>