<!-- Required options: branch build-flavor app-package-name package -->
<configuration description="Installs unbundled app, runs medium tests">
    <include name="google/unbundled/common/app-func" />
    <option name="test-tag" value="MediumSuite" />
    <option name="wifi:skip" value="true" />

    <test class="com.android.tradefed.testtype.InstalledInstrumentationsTest" >
        <option name="size" value="medium" />
        <option name="test-timeout" value="30000" />
    </test>

    <!-- result reporter options -->
    <!--  1 is medium -->
    <option name="sponge:sponge-size" value="1" />
</configuration>
