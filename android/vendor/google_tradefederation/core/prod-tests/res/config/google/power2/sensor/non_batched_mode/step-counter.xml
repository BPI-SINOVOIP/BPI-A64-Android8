<!-- Required input: monsoon_serialno monsoonserialno -->
<configuration description="Downloads build, flashes on device, and runs power test">
   <include name="google/power2/sensor/step-counter-base" />
   <option name="test-tag" value="powerStepCounterNonBatched" />
   <target_preparer class="com.android.tradefed.targetprep.KeyValueConfigPreparer" >
        <option name="path" value="${EXTERNAL_STORAGE}/PowerTests.conf" />
        <option name="separator" value="=" />
        <option name="config" key="TestCase" value="StepCounter" />
        <option name="config" key="TestCase_idle" value="StepCounterIdle" />
        <option name="config" key="Rate" value="40000" />
        <option name="config" key="BatchLatency" value="0" />
        <option name="config" key="TestDuration" value="900000" />
        <option name="config" key="IdleTestDuration" value="300000" />
    </target_preparer>
</configuration>
