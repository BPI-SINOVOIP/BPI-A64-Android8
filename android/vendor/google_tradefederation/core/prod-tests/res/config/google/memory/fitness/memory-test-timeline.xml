<configuration description="Downloads build, flashes on device, and runs memory tests">
    <option name="test-tag" value="FitMemoryTest" />
    <target_preparer class="com.android.tradefed.targetprep.InstallApkSetup">
    </target_preparer>
    <test class="com.google.android.memory.MemoryTestRunner">
        <option name="test-class-name" value="SystemHealthTest" />
        <option name="test-case-name" value="testMemoryTimeline" />
        <option name="test-package-name" value="com.google.android.apps.fitness.systemhealth" />
        <option name="metrics-key-name" value="fit.systemhealth.timeline" />
    </test>
</configuration>