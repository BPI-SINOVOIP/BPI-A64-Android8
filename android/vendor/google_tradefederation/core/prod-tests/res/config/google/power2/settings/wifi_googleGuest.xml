<!-- Required input: monsoon_serialno monsoonserialno -->
<configuration description="Set up the device for connecting to GoogleGuest AP">
    <include name="google/common/asit" />
    <include name="google/power2/settings/power-base" />
    <option name="google-device-setup:secure-setting" key="wifi_on" value="1" />
    <option name="google-device-setup:secure-setting" key="location_providers_allowed" value="none" />
    <option name="google-device-setup:enable-airplane-mode" value="true" />
    <option name="google-device-setup:wifi-network" value="GoogleGuest" />
    <option name="run-command"
        value="settings put secure location_providers_allowed -gps" />
    <option name="run-command"
        value="settings put secure location_providers_allowed -network" />
</configuration>
