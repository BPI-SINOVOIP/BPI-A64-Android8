<!-- Required input: monsoon_serialno monsoonserialno -->
<configuration description="Downloads build, flashes on device, and runs power test">
    <include name="google/power2/settings/gps_wifi" />
    <option name="test-tag" value="powerGpsprovider" />
    <target_preparer class="com.android.tradefed.targetprep.KeyValueConfigPreparer" >
        <option name="path" value="/data/local/tmp/PowerTests.conf" />
        <option name="separator" value="=" />
        <option name="config" key="TestCase" value="GpsLocationProvider" />
        <option name="config" key="TestDuration" value="3600000" />
    </target_preparer>
    <test class="com.google.android.power.tests.PowerTestRunner">
        <option name="report_unit_mw" value="true" />
        <option name="test_case_name" value="testGPSTenMinInterval" />
        <option name="test_class_name" value="BaseTests" />
        <option name="test_package_name" value="com.google.android.powertests" />
        <option name="test_runner_name" value="android.test.InstrumentationTestRunner" />
        <option name="monsoon_voltage" value="4.2" />
        <option name="monsoon_samples" value="39000" />
        <option name="ru_name" value="powerLocation" />
    </test>
</configuration>
