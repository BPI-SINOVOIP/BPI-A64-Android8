<configuration description="A test to check ATP lab wifi">

    <!-- reboot to ensure device to be in a good state. -->
    <target_preparer class="com.android.tradefed.targetprep.TimeWaster">
        <option name="delay" value="0" />
        <option name="reboot-before-sleep" value="true" />
    </target_preparer>

    <target_preparer class="com.android.tradefed.targetprep.WifiPreparer" />

    <logger class="com.android.tradefed.log.FileLogger" />
    <log_saver class="com.android.tradefed.result.FileSystemLogSaver">
        <option name="log-file-path" value="/auto/android-test/www/test_logs/atp" />
        <option name="log-file-url" value="http://android-test/www/test_logs/atp" />
        <option name="log-retention-days" value="14" />
    </log_saver>
    <result_reporter class="com.google.android.tradefed.result.SpongeResultReporter" />

</configuration>
