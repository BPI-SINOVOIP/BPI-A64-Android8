;-------------------------------------------------------------------------------
; How to config a test module
;
; [example]
; display_name= "Example"
; activated   = 1
; program     = "example.sh"
; category    = 0
; run_type    = 1
;
; display_name
;   The name show in the interface,it is a string.If you need to show the "",you can config it in this way:
;       display_name= string:"Example"
;   All the characters after "string:" will be show.
;   No more then 64 characters or 32 chinese in this item.
;   If no display_name , the test program will not run.
;
; activated
;   0: not test
;   1: test
;
; program
;   The program to test the module,no more then 16 characters
;
; category
;   0: automated testing
;   1: manual testing
;
; run_type
;   0: Start the next program after this program is completed
;   1: Start the next program ,do not need to wait for this program
;   Note:If category = 1, this item will be ignore
;-------------------------------------------------------------------------------


;-------------------------------------------------------------------------------
; test_size
;   unit: MB
;-------------------------------------------------------------------------------
[dram]
display_name= "DRAM"
activated   = 1
program     = "memtester.sh"
category    = 0
run_type    = 1
dram_size   = 2048
test_size   = 8

[rtc]
display_name= "RTC"
activated   = 1
program     = "rtctester.sh"
category    = 0
run_type    = 1

;-------------------------------------------------------------------------------
; module_path
;   The full path of wifi module file.
; module_args
;   The arguments of wifi module.
;-------------------------------------------------------------------------------
[wifi]
display_name= "WIFI"
activated   = 1
program     = "wifitester.sh"
category    = 0
run_type    = 1
module_count= 1
module_path = "/system/vendor/modules/bcmdhd.ko"
module_args =

