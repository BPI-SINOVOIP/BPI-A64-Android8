twi0_scl         = port:PH02<2><default><default><default>
twi0_sda         = port:PH03<2><default><default><default>

[twi1]
twi1_used        = 1
twi1_scl         = port:PH04<2><default><default><default>
twi1_sda         = port:PH05<2><default><default><default>

[twi2]
twi2_used        = 0
